{
  "name": "@player1os/javascript-support",
  "version": "1.3.14",
  "description": "A set of common utilities and scripts to be used in the development of projects written in Javascript",
  "main": "lib/index.js",
  "types": "@types/index.d.ts",
  "bin": {
    "support-require": "lib/runtime/task.js"
  },
  "scripts": {
    "preversion": "node lib/runtime/task.js ../task/preversion",
    "postversion": "node lib/runtime/task.js ../task/postversion",
    "test": "eslint . && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Player1os/javascript-support.git"
  },
  "keywords": [
    "javascript",
    "eslint",
    "config",
    "spawn",
    "process",
    "async"
  ],
  "author": "Osama Hassanein <osama.e.hassanein@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Player1os/javascript-support/issues"
  },
  "homepage": "https://github.com/Player1os/javascript-support/#readme",
  "engines": {
    "node": "^10.15.3",
    "npm": "^6.9.0"
  },
  "dependencies": {
    "bluebird": "^3.5.4",
    "colors": "^1.3.3",
    "eslint": "^6.7.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "module-alias": "^2.2.0"
  },
  "devDependencies": {
    "jest": "^24.7.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^\\.\\.\\.(.*)": "<rootDir>$1"
    },
    "testEnvironment": "node"
  }
}
