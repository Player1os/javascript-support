{
	"extends": [
		"eslint:recommended",
		"standard"
	],
	"rules": {
		"comma-dangle": ["error", "always-multiline"],
		"indent": ["error", "tab"],
		"max-len": ["error", { "code": 140 }],
		"no-tabs": ["off"],
		"object-curly-spacing": ["error", "always"],
		"operator-linebreak": ["error", "before"]
	}
}
